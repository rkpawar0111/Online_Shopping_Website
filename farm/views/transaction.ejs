<link rel="stylesheet" href="/style1.css">
<html>
	<head>
		<title>TRANSACTION-HISTORY</title>
	</head>
<h1 style="color:white;text-align:center;background-color:red;">
	 LAST TRANSACTIONS
</h1>
	<br style="line-height:50px;">
<body>
		<div>
		<table align="center" border="1">
			<thead>
			  <tr style="background-color:red;color:white;">
				<th>PRODUCT NAME</th>
				<th>PRODUCT PRICE</th>
				<th>PRODUCT QUANTITY</th>
				<th>GRAND TOTAL</th>
			  </tr>
			</thead>
			<tbody style="text-align:center;">
				<% for( var ar in data) {%>
				<tr>
					<td><%=data[ar].pname%></td>
					<td><%=data[ar].price%></td>
					<td><%=data[ar].quantity%></td>
					<td><%=data[ar].total%></td>
				</tr>
				<% } %>
			</tbody>
			</table>
		</div>
	<br style="line-height:30px;">
	<form method="post" action="/homefront1/<%=cid%>">
		<a>
			<button>BACK</button>
		</a>
	</form>
</body>
</html>